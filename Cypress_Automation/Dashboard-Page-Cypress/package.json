{
  "name": "kle-parents-portal-tests",
  "version": "1.0.0",
  "description": "Cypress test automation for KLE Technological University Parents Portal",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "open": "cypress open",
    "headed": "cypress run --headed",
    "chrome": "cypress run --browser chrome",
    "report:mochawesome": "cypress run --reporter mochawesome",
    "report:allure": "cypress run --env allure=true",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "keywords": [
    "cypress",
    "automation",
    "testing",
    "KLE",
    "parents-portal"
  ],
  "author": "Arun Kambrekar",
  "license": "MIT",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "cypress": "^13.6.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-multi-reporters": "^2.0.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
